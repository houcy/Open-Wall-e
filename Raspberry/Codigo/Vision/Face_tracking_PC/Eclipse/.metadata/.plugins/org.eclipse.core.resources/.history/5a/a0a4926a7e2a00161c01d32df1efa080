/////////////////////////////////////////////////
// trace if TRACE==1
/////////////////////////////////////////////////
inline void trace(string s)
{
	printf ("\n\c TRACE: %s", s);
}



/////////////////////////////////////////////////
// debug if DEBUG==1
/////////////////////////////////////////////////
inline void debug(string s)
{
	printf ("\n\c DEBUG: %s", s);
}
